<Application x:Class="TWBuildingAssistant.Actor.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:TWBuildingAssistant.Actor"
             xmlns:viewmodels="clr-namespace:TWBuildingAssistant.Actor.ViewModels"
             xmlns:views="clr-namespace:TWBuildingAssistant.Actor.Views">
    <Application.Resources>
        <ResourceDictionary>
            <DataTemplate DataType="{x:Type viewmodels:SettingsViewModel}">
                <views:SettingsView/>
            </DataTemplate>
            <DataTemplate DataType="{x:Type viewmodels:ProvinceViewModel}">
                <views:ProvinceView/>
            </DataTemplate>
            <DataTemplate DataType="{x:Type viewmodels:RegionViewModel}">
                <views:RegionView/>
            </DataTemplate>
            <DataTemplate DataType="{x:Type viewmodels:SlotViewModel}">
                <views:SlotView/>
            </DataTemplate>
        </ResourceDictionary>
    </Application.Resources>
</Application>
