{
  "$schema": "http://json-schema.org/draft-04/schema",
  "title": "JSON Schema for Factions aggregate",
  "type": "object",
  "properties": {
    "PublicOrder": {
      "type": "integer"
    },
    "ResearchRate": {
      "type": "integer"
    },
    "Growth": {
      "type": "integer"
    },
    "Fertility": {
      "type": "integer"
    },
    "ReligiousOsmosis": {
      "type": "integer"
    },
    "Food": {
      "type": "integer"
    },
    "Sanitation": {
      "type": "integer"
    },
    "TaxRate": {
      "type": "integer"
    },
    "CorruptionRate": {
      "type": "integer"
    },
    "Bonuses": {
      "type": "array",
      "minItems": 1,
      "uniqueItems": true,
      "items": {
        "$ref": "#/definitions/bonus"
      }
    },
    "Influences": {
      "type": "array",
      "minItems": 1,
      "uniqueItems": true,
      "items": {
        "$ref": "#/definitions/influence"
      }
    }
  },
  "additionalProperties": false,
  "definitions": {
    "bonus": {
      "type": "object",
      "properties": {
        "Value": {
          "type": "integer"
        },
        "Category": {
          "type": "integer",
          "minimum": 1,
          "maximum": 7
        }
      },
      "required": [ "Value" ],
      "additionalProperties": false
    },
    "influence": {
      "type": "object",
      "properties": {
        "Value": {
          "type": "integer"
        },
        "ReligionId": {
          "type": "string",
          "minLength": 1
        }
      },
      "required": [ "Value" ],
      "additionalProperties": false
    }
  }
}