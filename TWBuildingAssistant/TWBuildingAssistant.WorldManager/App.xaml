<Application xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:TWBuildingAssistant.WorldManager"
             xmlns:viewmodels="clr-namespace:TWBuildingAssistant.WorldManager.ViewModels"
             xmlns:views="clr-namespace:TWBuildingAssistant.WorldManager.Views"
             x:Class="TWBuildingAssistant.WorldManager.App">
    <Application.Resources>
        <ResourceDictionary>
            <DataTemplate DataType="{x:Type viewmodels:Resources.AddViewModel}">
                <views:Resources.AddView/>
            </DataTemplate>
            <DataTemplate DataType="{x:Type viewmodels:Resources.ListViewModel}">
                <views:Resources.ListView/>
            </DataTemplate>
        </ResourceDictionary>
    </Application.Resources>
</Application>
